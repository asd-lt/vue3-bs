{
    "name": "vue3-bs",
    "version": "1.0.11",
    "description": "Vue 3 + Bootstrap 5 Component Library",
    "repository": {
        "type": "git",
        "url": "https://github.com/asd-lt/vue3-bs.git"
    },
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "registry": "https://registry.npmjs.org/"
    },
    "main": "./dist/vue3-bs.umd.js",
    "module": "./dist/vue3-bs.mjs",
    "exports": {
        ".": {
            "import": "./dist/vue3-bs.mjs",
            "require": "./dist/vue3-bs.umd.js"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint . --fix",
        "format": "prettier --write .",
        "test": "vitest"
    },
    "keywords": [
        "vue",
        "vue3",
        "bootstrap",
        "bootstrap5",
        "components"
    ],
    "author": "",
    "license": "MIT",
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.15.0",
        "@vitejs/plugin-vue": "^6.0.4",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/test-utils": "^2.4.6",
        "bootstrap": "^5.3.8",
        "eslint": "^9.39.2",
        "eslint-plugin-vue": "^10.7.0",
        "globals": "^17.3.0",
        "jsdom": "^28.0.0",
        "prettier": "^3.8.1",
        "sass": "^1.97.3",
        "vite": "^7.3.1",
        "vitest": "^4.0.18",
        "vue": "^3.5.27"
    }
}
